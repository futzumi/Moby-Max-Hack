          d = b.encoding;
        if (d) {
          d = d.split("/");
          var e, f = d.length,
            p = b.data;
          try {
            for (; 0 < (e = f);) {
              var g = d[--f].match(/([\-\w]+)(\+([\w\-]+))?/);
              if (!g) break;
              var v = g[1];
              switch (v) {
                case "base64":
                  p = y.base64Decode(String(p));
                  e == d.length && (c = p);
                  continue;
                case "utf-8":
                  p = y.utf8Decode(p);
