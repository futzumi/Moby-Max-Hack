  centerPointY: 0.5,
  shadow: false,
  shadowOffsetX: 2,
  shadowOffsetY: 2,
  shadowBlur: 3,
  shadowColor: "#000",
  animate: false,
  distance: 15,
  easing: "swing",
  speed: 200,
  cssClass: "",
  cssStyles: {},
  activeClass: "bt-active",
  contentSelector: "$(this).attr('title')",
  ajaxPath: null,
  ajaxError: "<strong>ERROR:</strong> <em>%error</em>",
  ajaxLoading: "<blink>Loading...</blink>",
  ajaxData: {},
  ajaxType: "GET",
  ajaxCache: true,
  ajaxOpts: {},
  preShow: function() {
    return
  },
  postShow: function() {
    return
  },
  preHide: function() {
    return
  },
  postHide: function() {
    return
  },
  hoverIntentOpts: {
    interval: 300,
