      this.emit("disposed")
    };
    b.prototype.onWsError = function(b) {
      d.logAction(d.LOG_MINOR, "WebSocketTransport.onError()", "Error from WebSocket: " + b.message);
      var a = this;
      k.nextTick(function() {
        a.disconnect(b)
      })
    };
    b.prototype.dispose = function() {
      d.logAction(d.LOG_MINOR, "WebSocketTransport.dispose()", "");
