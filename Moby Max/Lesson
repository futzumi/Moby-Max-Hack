      }
    };
    b.prototype.onWsOpen = function() {
      d.logAction(d.LOG_MINOR, "WebSocketTransport.onWsOpen()", "opened WebSocket");
      this.emit("wsopen")
    };
    b.prototype.onWsClose = function(b) {
